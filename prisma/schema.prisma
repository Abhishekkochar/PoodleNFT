// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./devs.db"
}

model NftData{
  tokenId Int @id @unique
  userAddress String
  receipt String @unique
}